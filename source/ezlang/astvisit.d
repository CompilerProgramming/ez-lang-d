module ezlang.astvisit;

import ezlang.ast;

interface ASTVisitor {
    ASTVisitor enter(Program program);
    void exit(Program program);
    ASTVisitor enter(FuncDecl funcDecl);
    void exit(FuncDecl funcDecl);
    ASTVisitor enter(StructDecl structDecl);
    void exit(StructDecl structDecl);
    ASTVisitor enter(VarDecl varDecl);
    void exit(VarDecl varDecl);
    ASTVisitor enter(BinaryExpr binaryExpr);
    void exit(BinaryExpr binaryExpr);
    ASTVisitor enter(UnaryExpr unaryExpr);
    void exit(UnaryExpr unaryExpr);
    ASTVisitor enter(GetFieldExpr fieldExpr);
    void exit(GetFieldExpr fieldExpr);
    ASTVisitor enter(SetFieldExpr fieldExpr);
    void exit(SetFieldExpr fieldExpr);
    ASTVisitor enter(CallExpr callExpr);
    void exit(CallExpr callExpr);
    ASTVisitor enter(SimpleTypeExpr simpleTypeExpr);
    void exit(SimpleTypeExpr simpleTypeExpr);
    ASTVisitor enter(NullableSimpleTypeExpr simpleTypeExpr);
    void exit(NullableSimpleTypeExpr simpleTypeExpr);
    ASTVisitor enter(ArrayTypeExpr arrayTypeExpr);
    void exit(ArrayTypeExpr arrayTypeExpr);
    ASTVisitor enter(NullableArrayTypeExpr arrayTypeExpr);
    void exit(NullableArrayTypeExpr arrayTypeExpr);
    ASTVisitor enter(ReturnTypeExpr returnTypeExpr);
    void exit(ReturnTypeExpr returnTypeExpr);
    ASTVisitor enter(LiteralExpr literalExpr);
    void exit(LiteralExpr literalExpr);
    ASTVisitor enter(ArrayLoadExpr arrayLoadExpr);
    void exit(ArrayLoadExpr arrayLoadExpr);
    ASTVisitor enter(ArrayStoreExpr arrayStoreExpr);
    void exit(ArrayStoreExpr arrayStoreExpr);
    ASTVisitor enter(NewExpr newExpr);
    void exit(NewExpr newExpr);
    ASTVisitor enter(InitExpr initExpr);
    void exit(InitExpr initExpr);
    ASTVisitor enter(NameExpr nameExpr);
    void exit(NameExpr nameExpr);
    ASTVisitor enter(BreakStmt breakStmt);
    void exit(BreakStmt breakStmt);
    ASTVisitor enter(ContinueStmt continueStmt);
    void exit(ContinueStmt continueStmt);
    ASTVisitor enter(ReturnStmt returnStmt);
    void exit(ReturnStmt returnStmt);
    ASTVisitor enter(IfElseStmt ifElseStmt);
    void exit(IfElseStmt ifElseStmt);
    ASTVisitor enter(WhileStmt whileStmt);
    void exit(WhileStmt whileStmt);
    ASTVisitor enter(VarStmt varStmt);
    void exit(VarStmt varStmt);
    ASTVisitor enter(BlockStmt blockStmt);
    void exit(BlockStmt blockStmt);
    ASTVisitor enter(VarDeclStmt varDeclStmt);
    void exit(VarDeclStmt varDeclStmt);
    ASTVisitor enter(ExprStmt exprStmt);
    void exit(ExprStmt exprStmt);
    ASTVisitor enter(AssignStmt assignStmt);
    void exit(AssignStmt assignStmt);
}